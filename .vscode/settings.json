// 既定の設定とユーザー設定を上書きするには、このファイル内に設定を挿入します
{
    "typescript.tsdk": "node_modules/typescript/lib",
    // ファイルとフォルダーを除外するための glob パターンを構成します。
    "files.exclude": {
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.map": {
            "when": "$(basename)"
        }
    },
    "editor.formatOnSave": true
}